@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<h1>All projects:</h1>
<h3>@Model.CurrentCategory</h3>
<div class="row mt-5 mb-2">
    @{
        foreach (var proj in Model.AllProjects)
        {
        <form class="col-lg-4">
            <img src="@proj.Img" height="200" alt="@proj.Name" />
            <div class="form-group">
                <input type="submit" value="@proj.Name" name="projectName" asp-controller="Projects" asp-action="ThisProject" class="btn btn-outline-dark" />
            </div>
            <p>Desc: @proj.ShortDescription</p>
        </form>
        }
    }
</div>

